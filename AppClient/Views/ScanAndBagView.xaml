<rxui:ReactiveUserControl
    x:Class="AppClient.ScanAndBagView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ac="clr-namespace:AppClient"
    xmlns:rxui="http://reactiveui.net"
    x:TypeArguments="ac:ScanAndBagViewModel"
    Background="Transparent">

    <Grid>

        <StackPanel Margin="10" VerticalAlignment="Center">

            <TextBlock Margin="3" TextAlignment="Center">Scan And Bag</TextBlock>
            <Button x:Name="PickListButton" Content="Quick Pick" />

        </StackPanel>

    </Grid>
</rxui:ReactiveUserControl>
